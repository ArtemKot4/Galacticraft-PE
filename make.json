{
	"$schema": "https://raw.githubusercontent.com/zheka2304/innercore-mod-toolchain/develop/toolchain/schemas/make.schema.json",
	"resources": [
		{
			"type": "minecraft_resource_pack",
			"path": "minecraft_packs/galacticraft_resource"
		},
		{
			"type": "minecraft_behavior_pack",
			"path": "minecraft_packs/galacticraft_behavior"
		},
		{
			"path": "assets",
			"type": "resource_directory"
		},
		{
			"path": "models",
			"type": "resource_directory"
		},
		{
			"path": "assets/gui",
			"type": "gui"
		},
		{
			"path": "assets/environment",
			"type": "gui"
		}
	],
	"sources": [
		{
			"source": "dev",
			"target": "main.js",
			"type": "main",
			"language": "typescript"
		},
		{
			"source": "launcher.js",
			"type": "launcher",
			"language": "javascript"
		},
		{
			"source": "lib/*",
			"type": "library",
			"language": "javascript"
		}
	],
	"compile": [],
	"additional": [
		{
			"source": "config.json",
			"targetDir": "."
		},
		{
			"source": "config.info.json",
			"targetDir": "."
		}
	],
	"info": {
		"name": "Galacticraft 4 PE",
		"author": "Artem",
		"version": "alpha pre-release 1.0.0",
		"description": "",
		"icon": "mod_icon.png",
		"project": {
			"compiledLibraries": true
		}
	},
	"project": {
		"compiledLibraries": true
	},
	"api": "CoreEngine",
	"linkNative": ["minecraftpe", "innercore", "nativejs"],
	"excludeFromRelease": [],
	"tsconfig": {
		"jsx": "react",
		"jsxFactory": "UI.JSX.parse",
		"jsxFragmentFactory": "UI.JSX.Fragment",
		"moduleResolution": "nodenext"
	}
}
